{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "suiteplugins/um-login",
  "version": "1.0.0",
  "title": "Login Block for Ultimate Member",
  "category": "widgets",
  "icon": "smiley",
  "attributes": {
    "form_id": {
      "type": "string",
      "default": 0
    },
    "title": {
      "type": "string",
      "default": "Log In"
    },
    "show_avatar": {
      "type": "string",
      "default": true
    },
    "show_account": {
      "type": "string",
      "default": true
    },
    "show_logout": {
      "type": "string",
      "default": true
    },
    "show_edit_profile": {
      "type": "string",
      "default": true
    },
    "show_profile_url": {
      "type": "string",
      "default": true
    },
    "show_profile_tabs": {
      "type": "string",
      "default": true
    }
  },
  "keywords": [
    "login",
    "ultimate member",
    "um",
    "member",
    "form",
    "logout",
    "log in",
    "sign in",
    "sign out"
  ],
  "description": "Shows a login form for Ultimate Member users.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "login-widget-for-ultimate-member",
  "editorScript": "file:./index.js",
  "style": "file:./style.css"
}